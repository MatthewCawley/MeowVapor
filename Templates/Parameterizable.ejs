// MARK: Parameterizable.ejs
extension <%- serializable.name %> : Parameterizable {
	public static var uniqueSlug: String {
		return "<%- underscoreCase(uncamel(serializable.name)) %>"
	}

	public static func make(for parameter: String) throws -> <%- serializable.name %> {
		return try Meow.Helpers.requireValue(<%- serializable.name %>.findOne("_id" == ObjectId(parameter)), keyForError: "<%- serializable.name %> from URL parameter")
	}
}
